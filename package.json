{
  "name": "@lasercat/seelaude",
  "version": "1.2.0",
  "description": "A template for react-typescript components",
  "author": "Ryland Goldstein",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    "./ui": {
      "types": "./dist/types/ui/index.d.ts",
      "browser": "./dist/ui/index.js",
      "import": "./dist/ui/index.js",
      "default": "./dist/ui/index.js"
    },
    "./server": {
      "types": "./dist/types/server/index.d.ts",
      "node": "./dist/server/index.js",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lasercat-industries/seelaude.git"
  },
  "bugs": {
    "url": "https://github.com/lasercat-industries/seelaude/issues"
  },
  "homepage": "https://github.com/lasercat-industries/seelaude#readme",
  "scripts": {
    "prebuild": "bun run clean",
    "build": "bun ./scripts/build.ts && bun run gentypes",
    "gentypes": "bunx tsc --build --force tsconfig.build.json",
    "gentypes:ui": "bunx tsc --build --force ./src/ui/tsconfig.build.json",
    "gentypes:server": "bunx tsc --build --force ./src/server/tsconfig.build.json",
    "dev": "concurrently --kill-others-on-fail \"bun run dev:server\" \"bun run dev:client\" --names=server,ui -c cyan,magenta",
    "dev:client": "vite",
    "dev:server": "bun --watch run src/server/server.ts",
    "postbuild": "bun run build:validate",
    "build:analyze": "bun run build && bunx source-map-explorer dist/*.js",
    "build:check": "bun run build && bunx publint && bunx attw --pack",
    "build:size": "bun run build && bunx size-limit",
    "build:validate": "bun run scripts/validate-build.ts",
    "clean": "bun run scripts/clean.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path ./node_modules/@lasercat/config/.prettierignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path ./node_modules/@lasercat/config/.prettierignore",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@lasercat/config": "^1.2.3",
    "@tailwindcss/vite": "^4.1.12",
    "@vitejs/plugin-react": "^5.0.2",
    "dotenv": "^17.2.0",
    "eslint": "^9.30.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "class-variance-authority": "^0.7.1",
    "tailwind-merge": "^3.3.1",
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "tailwindcss": "^4.1.13",
    "react-router-dom": "^7.8.2"
  },
  "prettier": "@lasercat/config/.prettierrc.json",
  "peerDependencies": {
    "@types/bun": "latest",
    "@types/node": "24.x",
    "@types/react-dom": "^19.x",
    "hono": "4.x",
    "react": "^19.x",
    "react-dom": "^19.x",
    "react-router": "^7.x",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.108",
    "@types/ws": "^8.18.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  }
}
